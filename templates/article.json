{

    "name": "article Template",
    "wrapper": "div.templatearticle",
    "sections": {
        "uniquear-ticle":{
            "type": "template-article",

            "blocks": {
                "title":{
                    "type":"article_title"
                },
                "author":{
                    "type": "enable_authoor"
                },
                "published_date":{
                    "type": "published_date"
                },
                "thumbnail":{
                    "type":"featured_image"
                },
                "content":{
                    "type":"article_content"
                }



             
            },
            "block_order": ["thumbnail","title","published_date","author","content"]
        }
        
    },
    "order": ["uniquear-ticle"]
}


 